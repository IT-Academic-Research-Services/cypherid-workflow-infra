{
  "version": "2.0",
  "app_name": "cloudwatch-alerting",
  "autogen_policy": false,
  "lambda_memory_size": 256,
  "lambda_timeout": 600,
  "tags": {
    "managedBy": "chalice",
    "project": "idseq"
  },
  "environment_variables": {
